<?xml version="1.0" encoding="utf-8"?>

<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
        <variable
            name="personData"
            type="com.tarc.databindingbasic.Person" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity"><![CDATA[








        />















        ]]>

        <TextView
            android:id="@+id/name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="76dp"
            android:text="@={personData.name}"
            app:layout_constraintBottom_toBottomOf="@+id/editTextTextPersonName8"
            app:layout_constraintStart_toEndOf="@+id/editTextTextPersonName8"
            app:layout_constraintTop_toTopOf="@+id/editTextTextPersonName8"
            app:layout_constraintVertical_bias="0.545" />

        <TextView
            android:id="@+id/email"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="56dp"
            android:text="@={personData.ic}"
            app:layout_constraintBottom_toBottomOf="@+id/editTextTextPersonName9"
            app:layout_constraintStart_toEndOf="@+id/editTextTextPersonName9"
            app:layout_constraintTop_toTopOf="@+id/editTextTextPersonName9"
            app:layout_constraintVertical_bias="0.631" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="60dp"
            android:text="@={personData.email}"
            app:layout_constraintBottom_toBottomOf="@+id/editTextTextPersonName10"
            app:layout_constraintStart_toEndOf="@+id/editTextTextPersonName10"
            app:layout_constraintTop_toTopOf="@+id/editTextTextPersonName10" />

        <TextView
            android:id="@+id/address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="44dp"
            android:text="@={personData.address}"
            app:layout_constraintBottom_toBottomOf="@+id/editTextTextPersonName11"
            app:layout_constraintStart_toEndOf="@+id/editTextTextPersonName11"
            app:layout_constraintTop_toTopOf="@+id/editTextTextPersonName11"
            app:layout_constraintVertical_bias="0.666" />

        <Button
            android:id="@+id/btnUpdate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="72dp"
            android:text="Update"
            android:textAlignment="center"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName11"
            tools:layout_editor_absoluteX="135dp" />

        <EditText
            android:id="@+id/editTextTextPersonName8"
            android:layout_width="62dp"
            android:layout_height="41dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Name:"
            tools:layout_editor_absoluteX="1dp"
            tools:layout_editor_absoluteY="4dp" />

        <EditText
            android:id="@+id/editTextTextPersonName9"
            android:layout_width="83dp"
            android:layout_height="38dp"
            android:layout_marginTop="32dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="NRIC No:"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName8"
            tools:layout_editor_absoluteX="1dp" />

        <EditText
            android:id="@+id/editTextTextPersonName10"
            android:layout_width="75dp"
            android:layout_height="44dp"
            android:layout_marginTop="32dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Email:"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName9"
            tools:layout_editor_absoluteX="4dp" />

        <EditText
            android:id="@+id/editTextTextPersonName11"
            android:layout_width="90dp"
            android:layout_height="49dp"
            android:layout_marginTop="52dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Address:"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName10"
            tools:layout_editor_absoluteX="1dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>